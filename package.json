{
  "name": "dungeoneer",
  "productName": "Dungeoneer",
  "version": "1.2.6",
  "description": "A tool for Dungeon Masters running 5th edition D&D games.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never",
    "buildlinux": "electron-builder build --linux --publish never",
    "buildmac": "electron-builder build --mac --publish never",
    "deploy": "electron-builder build --win --publish always",
    "browsersync": "browser-sync start --server --files \"./*\""
  },
  "author": {
    "name": "Durtur",
    "email": "durtur@github.com",
    "url": "http://github.com/durtur/"
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "electron": "^22.3.25",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "electron-log": "^4.3.4",
    "electron-updater": "^4.1.2",
    "file-system": "^2.2.2",
    "howler": "^2.2.3",
    "marked": "^4.0.16",
    "nouislider": "^14.0.2",
    "quill": "^1.3.7",
    "semver": "^7.3.7",
    "sharp": "^0.30.5",
    "slim-select": "^1.27.0",
    "turndown": "^5.0.3",
    "uniqid": "^5.2.0"
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": {
    "url": "https://github.com/Durtur/Dungeoneer"
  }
}
